print ('Hitung luas ruangan')
p = float (input ('Masukkan Panjang Ruangan: '))
q = float (input ('Masukkan Lebar Ruangan: '))
r = input ('Masukkan satuan (Meter/Inci): ')
s = p * q
print ('Luas ruangan dengan panjang', p,'dan lebar', q, 'adalah: ', s,r)
